{
  "version": "2.1.0",
  "$schema": "https://json.schemastore.org/sarif-2.1.0.json",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "nono-core-minimal",
          "informationUri": "https://example.invalid/nono-core",
          "rules": [
            {
              "id": "demo-rule",
              "name": "demo-rule",
              "shortDescription": { "text": "Demo rule for acceptance pack" }
            }
          ]
        }
      },
      "results": [
        {
          "ruleId": "demo-rule",
          "level": "warning",
          "message": { "text": "Acceptance pack demo finding" },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": { "uri": "src/demo.js" },
                "region": { "startLine": 1, "startColumn": 1 }
              }
            }
          ]
        }
      ]
    }
  ]
}